type Query{}

type Mutation{}

interface Paginated{
    page: Int
    pageSize: Int
    total: Int
    totalPages: Int
    isFirstPage: Boolean
    isLastPage: Boolean
    sort: Sort
}

type Sort {
    empty: String
    sorted: Boolean
}

input SortInput {
    field: String!
    direction: SortDirection = ASC
}

enum SortDirection {
    ASC
    DESC
}